{
    "name": "front-end-terra",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint --ext .js --ext .jsx ./",
        "lint:fix": "eslint --fix --ext .js --ext .jsx ./",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "prepare": "husky install",
        "check-lint": "eslint --ext .js ./",
        "redux-devtools": "redux-devtools --hostname=localhost --port=8000"
    },
    "dependencies": {
        "@headlessui/react": "^1.3.0",
        "@heroicons/react": "^1.0.1",
        "@reduxjs/toolkit": "^1.6.0",
        "@terra-money/terra.js": "^1.8.0",
        "@terra-money/wallet-provider": "^1.2.0-alpha.1",
        "axios": "^0.21.1",
        "next": "10.2.3",
        "next-images": "^1.8.1",
        "prop-types": "^15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.9.0",
        "react-redux": "^7.2.4",
        "redux": "^4.1.0",
        "redux-thunk": "^2.3.0",
        "tailwindcss": "^2.1.4"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.14.4",
        "@babel/preset-react": "^7.13.13",
        "@redux-devtools/cli": "^1.0.0-9",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
        "autoprefixer": "^10.2.6",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.0.2",
        "enzyme": "^3.11.0",
        "eslint": "^7.28.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^6.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.0.4",
        "jsdom": "16.6.0",
        "jsdom-global": "3.0.2",
        "lint-staged": "^11.0.0",
        "moxios": "^0.4.0",
        "postcss": "^8.3.0",
        "prettier": "^2.3.1",
        "remote-redux-devtools": "^0.5.16"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "airbnb"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./**/*.{js,jsx,ts,tsx}": [
            "eslint . --fix"
        ]
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
        }
    }
}